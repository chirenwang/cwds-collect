<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wcc.wds.web.dao.WithdrawContributionDao">
  <select id="selectAll" resultType="com.wcc.wds.web.model.WithdrawContribution">
    select * from tb_withdraw_contribution
  </select>

  <select id="selectById" resultType="com.wcc.wds.web.model.WithdrawContribution">
    select * from tb_withdraw_contribution where id = #{id}
  </select>

  <insert id="insert" parameterType="com.wcc.wds.web.model.WithdrawContribution">
    insert into tb_withdraw_contribution(id,withdraw_type,withdraw_path,file_path)
    values (#{id},#{withdraw_type},#{withdraw_file_path},#{file_path})
  </insert>

  <delete id="deleteById">
    delete from tb_withdraw_contribution where id = #{id}
  </delete>
</mapper>