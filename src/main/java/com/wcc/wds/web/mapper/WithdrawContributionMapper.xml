<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wcc.wds.web.mapper.WithdrawContributionMapper">
  <select id="selectWithdraw" resultType="com.wcc.wds.bean.WithdrawContributionBean">
    select * from Blog
  </select>

  <select id="selectWithdrawById" resultType="com.wcc.wds.bean.WithdrawContributionBean">
    select * from Blog where id = #{id}
  </select>

  <insert id="insertWithdraw" parameterType="com.wcc.wds.bean.WithdrawContributionBean">
    insert into Blog(id,withdraw_type,withdraw_file_path,file_path)
    values (#{id},#{withdraw_type},#{withdraw_file_path},#{file_path})
  </insert>

  <delete id="deleteWithdraw" parameterType="com.wcc.wds.bean.WithdrawContributionBean">
    delete from Blog where id = #{id}
  </delete>
</mapper>